#include<iostream>
int main()
{
    for (int n = 0; n < 15; n++) // для вывода 15 кейсов при запуске
    {
        switch (n)
        {
        case(0):
            std::cout << "Message #1\n"; break;
        case(1):
            std::cout << "Message #2\n"; break;
        case(2):
            std::cout << "Message #3\n"; break;
        case(3):
            std::cout << "Message #4\n"; break;
        case(4):
            std::cout << "Message #5\n"; break;
        case(5):
            std::cout << "Message #6\n"; break;
        case(6):
            std::cout << "Message #7\n"; break;
        case(7):
            std::cout << "Message #8\n"; break;
        case(8):
            std::cout << "Message #9\n"; break;
        case(9):
            std::cout << "Message #10\n"; break;
        case(10):
            std::cout << "Message #11\n"; break;
        case(11):
            std::cout << "Message #12\n"; break;
        case(12):
            std::cout << "Message #13\n"; break;
        case(13):
            std::cout << "Message #14\n"; break;
        case(14):
            std::cout << "Message #15\n"; break;
        }
    }
}
