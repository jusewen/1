#include<iostream>
using namespace std;  //директивы using для всех идентификаторов в пространстве имен 
int fibonacci(int n)  // тип данных фибоначи
{
	if (n == 1 || n == 0) // тк f(0)=0 и f(1)=1   
		return(n);// условия завершения 
	else
		return(fibonacci(n - 1) + fibonacci(n - 2));  
}
int main()// переход к ввычислению 
{
	int n, x = 0;
	cout << "vvedi:";
	cin >> n;
		while (x <= n) 
		{
			cout << " " << fibonacci(x);
			x++;
		}   // для вывода по условию
	cout << " "; 
	return 0;
}
