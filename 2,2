#include<iostream>
using namespace std;  
int fibonacci(int n)  
{
	if (n == 1 || n == 0) // тк f(0)=0 и f(1)=1   
		return(n);
	else
		return(fibonacci(n - 1) + fibonacci(n - 2));  
}
int main()
{
	int n, x = 0;
	cout << "vvedi:";
	cin >> n;
		while (x <= n) 
		{
			cout << " " << fibonacci(x);
			x++;
		}   
	cout << " "; 
	return 0;
}
